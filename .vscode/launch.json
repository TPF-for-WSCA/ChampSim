{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C++ Launch",
            "type": "cppdbg",
            "request": "launch",
            "targetArchitecture": "x64",
            "program": "${workspaceRoot}/bin/champsim",
            "args": [
                "-warmup_instructions",
                "5000",
                "-simulation_instructions",
                "900000",
                "-result_dir",
                "~/Downloads/results/test_results",
                "-ptrace",
                // "-intel",
                "-traces",
                // "/home/robrunne/OneDrive/Research/datasets/traces/clang/clang.gz",
                "/home/robrunne/OneDrive/Research/datasets/traces/verilator/trace.gz"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [
                {
                    "name": "LD_LIBRARY_PATH",
                    "value": "/home/robrunne/Workspace/Simulators/xed/kits/xed-install-base-2022-11-08-lin-x86-64/lib"
                }
            ],
            "MIMode": "gdb",
            "preLaunchTask": "make DebugBuild"
        },
        {
            "name": "Python: MergingBlocks",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/aggregation_scripts/apply_merge_strategies.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "stopOnEntry": false,
            "args": [
                "~/Downloads/results"
            ]
        }
    ]
}